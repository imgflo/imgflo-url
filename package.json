{
  "name": "imgflo-url",
  "version": "1.1.0",
  "description": "Conveniently produce authorized imgflo URLs.",
  "main": "lib/imgflo-url.js",
  "keywords": [
    "imgflo"
  ],
  "homepage": "https://github.com/the-grid/imgflo-url",
  "bugs": "https://github.com/the-grid/imgflo-url/issues",
  "license": "MIT",
  "author": {
    "name": "Paul Young",
    "url": "https://github.com/paulyoung"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-grid/imgflo-url.git"
  },
  "bin": {
    "imgflo-url": "bin/imgflo-url"
  },
  "scripts": {
    "build": "npm run clean; ./node_modules/.bin/coffee --compile --output lib/ src/",
